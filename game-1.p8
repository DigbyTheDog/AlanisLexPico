pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
player = {
	x = 60,
	y = 1,
	sprite = 3,
	speed = 2,
	moving = false
}

count = 0

function _init()

end

function _update() 

	move()
 
end

function _draw()

 cls()
	camera(0,0)
	map(0,0,0,0,128,32)
	spr(getplayersprite(), player.x, player.y)
	
end

function move()

	player.moving = false
	
 -- left
 if (btn(0)) then
 	player.x = player.x - 1
 	player.moving = true
 end
 -- right
 if (btn(1)) then
 	player.x = player.x + 1
 	player.moving = true
 end
 -- up
 if (btn(2)) then
 	player.y = player.y - 1
 	player.moving = true
 end
 -- down
 if (btn(3)) then
 	player.y = player.y + 1
 	player.moving = true
 end
 
end


function getplayersprite()

	if(player.moving) then
	 if(count % 10 == 0) then
			print(count)
		end
		count = count + 1
	else 
		return 3
	end
	
	return 3
	
end
